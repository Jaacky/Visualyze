<!DOCTYPE html>  
<html lang="en">  
    <head>
        <%- include('partials/header.ejs') %>
        <script src="/js/dataset.js"></script>
        <script src="/js/graph.js"></script>
    </head>

    <script>
        var graphs = <%- JSON.stringify(graphs) %>;
        console.log(graphs);
    </script>

    <body>
        <div>
            <h2> User id: <%= user.email %> </h2>
        </div>

        <div class="row">
            <% for (var i=0; i<graphs.length; i++) { %>
                <div class="col-sm-4">
                    <a href="/graph/<%= graphs[i].id %>"><button class="btn btn-default"><%= graphs[i].name %></button></a>
                </div>
            <% } %>
        </div>
    </body>
</html>  